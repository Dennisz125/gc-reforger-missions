  "b. Signal:"\
  "COY:47MHz"\
  "1PLT: 48MHz"\
  "2PLT 49MHz"\
  ""\
  ""\
  "Callsigns:"\
  "1PLT 1'6, 1'1,1'2,1'3"\
  "2PLT 2'6, 2'1.2'2,2'3"
